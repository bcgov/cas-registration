{
  "name": "administration-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/administration-e2e/src",
  "tags": [],
  "implicitDependencies": ["administration"],
  "// targets": "to see all targets run: nx show project administration-e2e --web",
  "targets": {
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/administration-e2e",
        "command": "happo-e2e -- playwright test"
      }
    },
    "e2e:ui": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/administration-e2e",
        "command": "happo-e2e -- playwright test --ui"
      }
    },
    "e2e:report": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/administration-e2e",
        "command": "playwright show-report"
      }
    }
  }
}
