{
  "extends": [
    "plugin:@nx/react-typescript",
    "next",
    "plugin:playwright/recommended",
    "../../.eslintrc.json"
  ],
  "ignorePatterns": ["!**/*"],
  "parserOptions": {
    "project": ["apps/dashboard-e2e/tsconfig(.*)?.json"],
    "tsConfigRootDir": "./"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {},
      "parser": "@typescript-eslint/parser"
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["src/**/*.{ts,js,tsx,jsx}"],
      "rules": {}
    }
  ]
}
