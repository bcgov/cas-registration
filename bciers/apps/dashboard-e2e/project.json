{
  "name": "dashboard-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/dashboard-e2e/src",
  "implicitDependencies": ["dashboard"],
  "// targets": "to see all targets run: nx show project dashboard-e2e --web",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {}
    },
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/dashboard-e2e",
        "command": "happo-e2e -- playwright test"
      }
    },
    "e2e:ui": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/dashboard-e2e",
        "command": "happo-e2e -- playwright test --ui"
      }
    },
    "e2e:report": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/dashboard-e2e",
        "command": "playwright show-report"
      }
    }
  }
}
