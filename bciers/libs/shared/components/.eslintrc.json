{
  "extends": ["plugin:@nx/react", "../../../.eslintrc.json", "prettier"],
  "globals": { "vi": true },
  "ignorePatterns": ["!**/*"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": ["libs/shared/components/tsconfig(.*)?.json"],
    "tsConfigRootDir": "./"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
