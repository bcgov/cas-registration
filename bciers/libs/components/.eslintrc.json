{
  "extends": ["plugin:@nx/react", "../../.eslintrc.json", "prettier"],
  "globals": {
    "vi": true
  },
  "ignorePatterns": [
    "!**/*",
    "**/vite.config.*.timestamp*",
    "**/vitest.config.*.timestamp*"
  ],
  "parserOptions": {
    "project": ["libs/components/tsconfig(.*)?.json"],
    "tsConfigRootDir": "./"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
